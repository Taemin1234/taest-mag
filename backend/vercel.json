{
  "version": 2,
  "regions": [
    "icn1"
  ],
  "functions": {
    "api/**/*": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    }
  ]
}